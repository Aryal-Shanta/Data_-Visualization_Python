    "#Calculating total character in any file including comma, space, newline and other all\n",
    "\n",
    "fn = open(\"File.txt\")\n",
    "now=fn.read()\n",
    "count=0\n",
    "data=now.split()\n",
    "print(data)\n",
    "for line in now:\n",
    "    word = line.split()\n",
    "    count=count+1\n",
    "    print(word)\n",
